server {
    listen 80 default_server;
    listen [::]:80 default_server;

    root /var/www/html;
    autoindex on;                   # 서버 리스팅

    index index.html index.htm;

    server_name _;

    location /rhel7.6 {
            root /;                 # /rhel7.6 와 연결된다.
            try_files $uri $uri/ =404;
    }

    location /flask {
        proxy_pass http://127.0.0.1:5000/;      # 역프록시 마지막에 / 필수
    }
}